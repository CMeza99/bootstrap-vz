{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Backports setup and package install",
	"type": "object",
	"properties": {
		"plugins": {
			"type": "object",
			"properties": {
				"backports": {
					"type": "object",
					"properties": {
						"packages": {
							"type": "array",
							"items": {
								"type": "string"
							}
						}
					},
					"required": ["packages"]
				}
			},
			"required": ["backports"]
		}
	},
	"required": ["plugins"]
}
