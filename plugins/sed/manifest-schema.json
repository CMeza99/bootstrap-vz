{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Backports setup and package install",
	"type": "object",
	"properties": {
		"plugins": {
			"type": "object",
			"properties": {
				"sed": {
					"type": "object",
					"properties": {
						"file": {
							"type": "string"
						},
						"find": {
							"type": "string"
						},
						"replace": {
							"type": "string"
						}
					}
				}
			},
			"required": ["sed"]
		}
	},
	"required": ["plugins"]
}
