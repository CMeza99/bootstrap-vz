{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Prebootstrapped plugin manifest",
	"type": "object",
	"properties": {
		"plugins": {
			"type": "object",
			"properties": {
				"prebootstrapped": {
					"type": "object",
					"properties": {
						"snapshot": {
							"type": "string"
						},
						"image": {
							"type": "string"
						}
					}
				}
			},
			"required": ["prebootstrapped"]
		}
	},
	"required": ["plugins"]
}
